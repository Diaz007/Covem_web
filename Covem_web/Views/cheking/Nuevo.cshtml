@model Covem_web.Vista_Cheking

@{
    ViewBag.Title = "Nuevo";
    Layout = "~/Views/Shared/_Layout_Principal.cshtml";
}

<h2>Nuevo</h2>

<div class="row">
    <div class="col-md-5 grid-margin grid-margin-md-0 stretch-card">
        <div class="card">
            <div class="card-body text-center">
                <div>
                    <img src=@Model.persona.Imagen class="img-lg rounded-circle mb-2" alt="profile image" />
                    <h4>@Model.persona.nombre @Model.persona.Apellidos</h4>
                    <p class="text-muted mb-0">@Model.persona.Licencia</p>
                </div>
                <p class="mt-2 card-text">
                    @Model.Marca.Marca1 @Model.modelo.modelo1  @Model.auto.año
                </p>

                <div class="border-top pt-3">
                    <div class="row">
                        <div class="col-4">

                        </div>
                        <div class="col-4">
                            <h6>@Model.auto.KilometrajeActual</h6>
                            <p>Ultimo Registro de Km</p>
                        </div>
                        <div class="col-4">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-7 grid-margin grid-margin-md-0 stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Documentos Activos</h4>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th class="pt-1 ps-0">
                                    Documento
                                </th>
                                <th class="pt-1">
                                    Estatus
                                </th>
                                <th class="pt-1">
                                    Vigencia
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @{
                                if (Model.lista.Count > 0)
                                {
                                    foreach (var item in Model.lista)
                                    {
                                        <tr>
                                            <td class="py-1 ps-0">
                                                <div class="d-flex align-items-center">

                                                    <div class="ms-3">
                                                        <p class="mb-0">@item.nombre</p>

                                                    </div>
                                                </div>
                                            </td>
                                            <td>
                                                @item.Vigencia.Value.ToShortDateString()

                                            </td>
                                            <td>
                                                @if (@item.Vigencia.Value < DateTime.Now)
                                                {<label class="badge badge-danger">Vencido</label> }
                                                else
                                                {<label class="badge badge-success">Activo</label>}
                                            </td>
                                        </tr>
                                    }
                                }
                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

